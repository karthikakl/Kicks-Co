            //search

            function myFunction() {
    const input = document.getElementById('myInput');
    const filter = input.value.toUpperCase();
    const products = document.querySelectorAll('.product-cart-wrap');
    let productFound = false;

    products.forEach(function(product) {
        const productName = product.querySelector("h2 a").textContent.toUpperCase();
        if (productName.indexOf(filter) > -1) {
            product.style.display = "";
            productFound = true; 
        } else {
            product.style.display = "none"; // Hide the product if it doesn't match the search
        }
    });

    if (!productFound) {
        document.getElementById('noProductsMessage').style.display = "block";
    } else {
        document.getElementById('noProductsMessage').style.display = "none";
    }
}

//filter

const selectElement = document.getElementById('statusFilter');
selectElement.addEventListener('change', function (event) {
    const selectedCategory = event.target.value.toLowerCase();
    const products = document.querySelectorAll('.product-cart-wrap');

    products.forEach(product => {
        const productCategory = product.querySelector(".product-category").textContent.toLowerCase().trim();

        if (selectedCategory === 'all categories' || selectedCategory === productCategory) {
            product.style.display = 'block';
        } else {
            product.style.display = 'none';
        }
    });
});


