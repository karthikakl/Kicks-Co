<body>
    <h1>Results Products</h1>
    <p>Selected Category: <%= category %></p> 
    

    <div class="container">
        <div class="row">
            <!-- Price Filtering -->
            <div class="col-lg-3">
                <div class="widget-category mb-30">
                    <h5 class="section-title style-1 mb-30 wow fadeIn animated">Sort by</h5>
                    <ul class="categories">
                        <li><a href="?sort=offerPrice-low-to-high" class="<%= sort === 'offerPrice-low-to-high' ? 'active' : '' %>">Price: Low to High</a></li>
                        <li><a href="?sort=offerPrice-high-to-low" class="<%= sort === 'offerPrice-high-to-low' ? 'active' : '' %>">Price: High to Low</a></li>
                    </ul>
                </div>
            </div>
            <!-- End Price Filtering -->
            
            <!-- Product Grid -->
            <div class="col-lg-9">
                <div class="tab-pane fade show active" id="tab-one" role="tabpanel" aria-labelledby="tab-one">
                    <div class="row product-grid-4">
                        <% if (products && products.length > 0) { %>
                            <% products.forEach(product => { %>
                                <div class="col-lg-4 col-md-4 col-12 col-sm-6">
                                    <div class="product-cart-wrap mb-30">
                                        <div class="product-img-action-wrap">
                                            <div class="product-img product-img-zoom">
                                                <a href="/productDetails?id=<%= product._id %>">
                                                    <img class="default-img" src="/product-images/<%= product.image[0].filename %>" alt="">
                                                    <img class="hover-img" src="/product-images/<%= product.image[1].filename %>" alt="">
                                                </a>
                                            </div>
                                        </div>
                                        <div class="product-content-wrap">
                                            <div class="product-category">
                                                <a href="shop-grid-right.html"><%= product.brand %></a>
                                            </div>
                                            <h2><a href="shop-product-right.html"><%= product.name %></a></h2>
                                            <div class="product-price">
                                                <span>₹
                                                    <%= product.offerPrice %></span>
                                                    <span class="old-price">₹
                                                        <%= product.orginalPrice %></span>
                                                    </div>
                                                    <div class="product-action-1 show">
                                                        <a aria-label="Add To Cart" class="action-btn hover-up" href="/products/:id/addWhislist">
                                                            <i class="fi-rs-shopping-bag-add"></i>
                                                        </a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    <% }) %>
                                <% } else { %>
                                    <div class="col-md-12">
                                        <img src="/product-images/noProduct.png" alt="noProduct" style="width: 500px; height: auto; display: block; margin: 0 auto;">
                                        <div id="noProductsMessage" style="font-size: 18px; color: red; text-align: center;">
                                            No products available at the moment.
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                    </div>
                    <!-- End Product Grid -->
                </div>
            </div>
            
                                </div>
</body>
